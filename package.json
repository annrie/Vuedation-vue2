{
  "name": "vuedation",
  "version": "1.5.0",
  "private": true,
  "description": "A Vue.js project use webpack5 and babel7",
  "author": "annrie <annrie@users.noreply.github.com>",
  "scripts": {
    "clean": "npx rimraf ./dist",
    "dev": "npm run webpack:dev && npm run webpack:server",
    "webpack:server": "npx webpack serve --config ./build/webpack.dev.js",
    "webpack:dev": "npm run clean && npx webpack --config ./build/webpack.dev.js",
    "build": "npm run clean && npx webpack --config ./build/webpack.prod.js",
    "lint": "eslint --ext .js,.vue src",
    "format:stylesheet": "prettier-stylelint --quiet --write src/assets/scss/**/*",
    "lint:stylesheet": "stylelint --fix src/assets/scss/**/*.scss",
    "format:all": "prettier --write ./**/*.{js,jsx,ts,tsx,md}",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.css*": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "add": "^2.0.6",
    "cz-customizable": "^6.3.0",
    "foundation-sites": "^6.7.0",
    "husky": "^7.0.2",
    "jquery": "^3.6.0",
    "lint-staged": "^11.1.2",
    "motion-ui": "git+https://github.com/annrie/motion-ui.git#main",
    "vue": "^2.6.12",
    "vue-head": "^2.2.0",
    "vue-mq": "^1.0.1",
    "vue-router": "^3.4.5",
    "vue-scrollto": "^2.18.2",
    "vuex": "^3.5.1",
    "what-input": "^5.2.10"
  },
  "peerDependencies": {
    "postcss": "^8.0.0",
    "webpack": "^4.0.0 || ^5.0.0",
    "fsevents": "^2.2 || ^2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/preset-env": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@soda/friendly-errors-webpack-plugin": "^1.8.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-eslint": "^10.1.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "7.0.1",
    "eslint-webpack-plugin": "^2.5.4",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.2.0",
    "postcss": "^8.3.6",
    "postcss-assets": "^6.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^4.0.0",
    "prettier": "^2.0.2",
    "prettier-eslint": "^9.0.1",
    "sass": "^1.38.1",
    "sass-loader": "^12.1.0",
    "sass-resources-loader": "^2.2.4",
    "script-loader": "^0.7.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.20.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "terser-webpack-plugin": "^5.1.4",
    "url-loader": "^4.1.1",
    "vue-eslint-parser": "7.10.0",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.3",
    "vue-svg-inline-loader": "^2.1.2",
    "vue-template-compiler": "2.6.12",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-fix-style-only-entries": "^0.6.1",
    "webpack-merge": "^5.8.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not ie <= 8"
            ]
          },
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ],
    "plugins": [
      "transform-vue-jsx"
    ]
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "resolutions": {
    "fsevents": "~2.1.2"
  }
}
